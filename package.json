{
  "name": "hinter-cline",
  "version": "0.2.4",
  "description": "AI-assisted environment for hinter operations",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hinter-net/hinter-cline.git"
  },
  "scripts": {
    "docker:build": "docker build -t bbenligiray/hinter-cline:latest .",
    "docker:start": "docker run -it --rm -p8080:8080 -v./hinter-core-data:/hinter-cline/hinter-core-data bbenligiray/hinter-cline:latest",
    "prettier": "prettier --write \"./**/*.{js,md}\"",
    "prettier:check": "prettier --check \"./**/*.{js,md}\"",
    "start": "node src/index.js",
    "test": "jest"
  },
  "bin": {
    "hinter-helper": "src/index.js"
  },
  "dependencies": {
    "jest": "^30.0.4",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "prettier": "^3.6.2"
  }
}
